{
  "name": "cardhost",
  "version": "0.0.0",
  "private": true,
  "description": "The standard clientside app for hosting Cardstack Cards",
  "repository": {
    "type": "git",
    "url": "https://github.com/cardstack/cardstack",
    "directory": "server"
  },
  "license": "MIT",
