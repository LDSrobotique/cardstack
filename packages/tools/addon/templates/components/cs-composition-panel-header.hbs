<div class="cs-composition-panel-header--info">
  {{cs-svg
    "article"
    width="22px"
    height="28px"
  }}
  <div>
    <p class="content-type">{{cs-humanize (cs-model-type model)}}</p>
    {{#if content.title}}
      <p class="content-title">
        {{#if model.created}}
          {{moment-format model.created "YYYY-MM-DD"}}
        {{/if}}
        {{title}}
      </p>
    {{/if}}
  </div>
</div>

{{cs-version-control
  model=model
  enabled=editingEnabled
  headerSectionFields=headerSectionFields
  afterModelSaved=afterModelSaved
  on-error=(action (mut validationErrors))
}}
