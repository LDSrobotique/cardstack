<header>Select Content Type</header>

<ul>
  {{#each availableTypes as |type|}}
    {{! template-lint-disable no-invalid-interactive }}
    <li data-test-cs-create-menu-item={{type.id}}>
      {{cs-humanize type.id}}
      <button onclick={{action "create" type}}>Add +</button>
    </li>
  {{/each}}
</ul>
