<div class="cs-card-picker-image-item">
  <div class="cs-card-picker-image-item--header"></div>
  <img
    class="cs-card-picker-image-item--image"
    src={{imageSrc}}
    alt={{content.altText}}
  >
  <div class="cs-card-picker-image-item--meta">
    <div class="cs-card-picker-image-item--filename">{{content.imageFileName}}</div>
    <div class="cs-card-picker-image-item--type">Image</div>
  </div>
</div>